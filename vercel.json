{
  "version": 2,
  "functions": {
    "api/proxy.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/api/proxy", "dest": "/api/proxy.py" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}
